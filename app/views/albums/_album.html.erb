<div id="<%= dom_id album %>">
  <p>
    <strong>Artist:</strong>
    <%= album.artist_id %>
  </p>

  <p>
    <strong>Albumtitle:</strong>
    <%= album.title %>
  </p>

  <p>
    <strong>Slug:</strong>
    <%= album.slug %>
  </p>

  <p>
    <strong>Release date:</strong>
    <%= album.release_date %>
  </p>

  <p>
    <strong>Cover:</strong>
    <%= album.cover %>
  </p>

  <p>
    <strong>Album notes:</strong>
    <%= album.album_notes %>
  </p>

  <p>
    <strong>Lastmod:</strong>
    <%= album.lastmod %>
  </p>

  <p>
    <strong>Lastuser:</strong>
    <%= album.lastuser %>
  </p>

</div>
